<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="centerPage" styleClass="global-color" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unical.elcampeon.controller.ProfileController">
   <children>
      <VBox alignment="CENTER" spacing="50.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" spacing="50.0">
               <children>
                  <VBox alignment="CENTER_LEFT" spacing="25.0">
                     <children>
                        <Label prefHeight="25.0" styleClass="label-class" text="Username">
                           <font>
                              <Font size="18.0" />
                           </font></Label>
                        <Label fx:id="nameLabel" prefHeight="25.0" styleClass="label-class" text="First name">
                           <font>
                              <Font size="18.0" />
                           </font></Label>
                        <Label fx:id="surnameLabel" prefHeight="25.0" styleClass="label-class" text="Last name">
                           <font>
                              <Font size="18.0" />
                           </font></Label>
                        <Label fx:id="emailLabel" layoutX="10.0" layoutY="77.0" prefHeight="25.0" styleClass="label-class" text="Email">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label fx:id="birthdayLabel" layoutX="10.0" layoutY="207.0" prefHeight="25.0" styleClass="label-class" text="Birthday">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER_LEFT" spacing="25.0">
                     <children>
                        <TextField fx:id="usernameTextField" prefHeight="25.0" prefWidth="200.0" style="-fx-background-radius: 10;" />
                        <TextField fx:id="firstTextField" prefHeight="25.0" prefWidth="200.0" style="-fx-background-radius: 10;" />
                        <TextField fx:id="lastTextField" prefHeight="25.0" prefWidth="200.0" style="-fx-background-radius: 10;" />
                        <TextField fx:id="emailTextField" layoutX="10.0" layoutY="80.0" prefHeight="25.0" prefWidth="200.0" style="-fx-background-radius: 10;" />
                        <TextField fx:id="birthdayTextField" layoutX="10.0" layoutY="205.0" prefHeight="25.0" prefWidth="200.0" style="-fx-background-radius: 10;" />
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" spacing="50.0">
               <children>
                  <Button fx:id="saveButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onSaveClick" textFill="WHITE">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                     <font>
                        <Font size="16.0" />
                     </font>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <graphic>
                        <Label fx:id="applyLabel" />
                     </graphic>
                  </Button>
                  <Button alignment="CENTER" contentDisplay="CENTER" layoutX="377.0" layoutY="28.0" mnemonicParsing="false" onAction="#onCancelClick" textFill="WHITE">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                     <font>
                        <Font size="16.0" />
                     </font>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <graphic>
                        <Label fx:id="cancelLabel" />
                     </graphic>
                  </Button>
                  <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onChangePasswordClick" textFill="WHITE">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                     <font>
                        <Font size="16.0" />
                     </font>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <graphic>
                        <Label fx:id="changePassLabel" />
                     </graphic>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</AnchorPane>
