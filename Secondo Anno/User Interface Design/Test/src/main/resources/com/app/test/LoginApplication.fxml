<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.test.LoginController">
   <children>
      <VBox prefWidth="300.0">
         <children>
            <HBox prefHeight="50.0" prefWidth="300.0" spacing="20.0">
               <children>
                  <Label text="Username" />
                  <TextField fx:id="user_label" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
               </padding>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="300.0" spacing="20.0">
               <children>
                  <Label text="Password" />
                  <PasswordField fx:id="pass_label" />
               </children>
               <padding>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
               </padding>
            </HBox>
            <Button mnemonicParsing="false" onAction="#do_login" prefHeight="25.0" prefWidth="90.0" text="Button">
               <VBox.margin>
                  <Insets bottom="25.0" left="105.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
